package teste;

public class banco {
    public <Conta> void main1 (String [] args) {
    	@SuppressWarnings("unused")
		int numero;
		@SuppressWarnings("unused")
		String cliente;
		@SuppressWarnings("unused")
		double saldo;
		@SuppressWarnings("unused")
		double limite;
    @SuppressWarnings("unused")
	 class conta {
    	@SuppressWarnings("unchecked")
		Conta conta = (Conta) new conta();
    	private double numero;
    	private String getNome() {
    		return cliente;
    	}
    	class conta extends banco {
    		private int numero;
    		private String cliente;
    	    private double saldo;
    	    private double limite;
    	    
    	    public String getNome()  {
    	    	return cliente;
    	    }
			public void setCliente (String nome) {
				cliente = "Bianca";			    	   
			     }
			public double getNumero() {
				return numero;
			}
			public void setNumero (double numero) {
				this.numero = 220;
			}
			public double getSaldo() {
			    return saldo;	 
			     }
			public void setSaldo (double saldo) {
			    	 this.saldo = saldo;
			         saldo = 1000.0;
			}
			public double getLimite() {
				return limite;
			}
			public void setLimite (double limite) {
				this.limite = 10000.0;
			}
			System.out.println(conta.getCliente());
			System.out.println(conta.getSaldo());
			System.out.println(conta.getLimite());
    	
    	class conta2 extends banco {
    	    public <Conta> void main (String[] args) {
			     Conta conta2;
			     int numero;
			     numero = 153;
			     String cliente;
